.content {
 background: white;
 width: 100%;
 padding: 10px;
 line-height: 22px;
 margin-right:0;
 -webkit-transition: all 0.4s;
 transition: all 0.4s;
 -webkit-transform: translateX(90px);
 transform: translateX(90px);
 margin-left: -90px;
 border-radius: 5px;
}

/* 绝对定位容器 */
.float-action, .float-action2 {
  position: fixed;
  bottom: 170rpx; /* 控制底部距离 */
  right: 80rpx;  /* 控制右侧距离 */
  width: 100rpx;
  height: 100rpx;
  z-index: 1000;
}

/* 第一个 icon 按钮的位置 */
.float-action2 {
  bottom: 330rpx; /* 相对于另一个 icon 的垂直间距 */
}

.float-action img, .float-action2 img {
  width: 100%;
  height: 100%;
}
