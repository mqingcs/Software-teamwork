/*! weui.js v1.1.0 (https://github.com/weui/weui-wxss) */
page {
  line-height: 1.6;
  font-family: -apple-system-font, "Helvetica Neue", sans-serif;
}
icon {
  vertical-align: middle;
}
.weui-cells {
  position: relative;
  margin-top: 1.17647059em;
  background-color: #FFFFFF;
  line-height: 1.41176471;
  font-size: 17px;
}
.weui-cells__title {
  margin-top: .77em;
  margin-bottom: .3em;
  padding-left: 15px;
  padding-right: 15px;
  color: #999999;
  font-size: 14px;
}
.weui-cells_after-title {
  margin-top: 0;
}
.weui-cell {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.weui-cell_primary {
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
          align-items: flex-start;
}
.weui-cell__bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.weui-cell__ft {
  text-align: right;
  color: #999999;
}
.weui-cell_input {
  padding-top: 0;
  padding-bottom: 0;
}
.weui-label {
  width: 105px;
  word-wrap: break-word;
  word-break: break-all;
}
.weui-input {
  height: 2.58823529em;
  min-height: 2.58823529em;
  line-height: 2.58823529em;
  border: 1px solid #e5e5e5;
  border-radius: 4px;
  padding-left: 10px;
  background-color: #f8f8f8;
}
.weui-textarea {
  display: block;
  width: 100%;
  padding: 10px;
  border: 1px solid #e5e5e5;
  border-radius: 4px;
  background-color: #f8f8f8;
}
.weui-textarea-counter {
  color: #B2B2B2;
  text-align: right;
}
.weui-cell_warn {
  color: #E64340;
}
.weui-select {
  position: relative;
  padding-left: 15px;
  padding-right: 30px;
  height: 2.58823529em;
  min-height: 2.58823529em;
  line-height: 2.58823529em;
  border: 1px solid #d9d9d9;
  border-radius: 4px;
  background-color: #f8f8f8;
}
.weui-btn {
  margin-top: 15px;
  background-color: #09bb07;
  color: #fff;
  padding: 10px;
  border-radius: 4px;
  text-align: center;
  font-size: 16px;
}
.weui-btn-area {
  margin: 1.17647059em 15px 0.3em;
}
.weui-btn_primary {
  background-color: #1AAD19;
  color: #FFFFFF;
}
.weui-btn_default {
  background-color: #f8f8f8;
  color: #333;
}
.weui-footer_fixed-bottom {
  position: fixed;
  bottom: .52em;
  left: 0;
  right: 0;
}
.weui-footer__link {
  display: inline-block;
  vertical-align: top;
  margin: 0 .62em;
  font-size: 14px;
  color: #586C94;
}
.weui-footer__text {
  padding: 0 .34em;
  font-size: 12px;
}

/* 位置选择器样式 */
.weui-location-picker {
  color: #999;
  padding: 10px;
  border: 1px solid #d9d9d9;
  border-radius: 4px;
  background-color: #f8f8f8;
  display: flex;
  align-items: center;
}
.weui-location-selected {
  color: #333;
  padding: 10px;
}
.weui-location-placeholder {
  color: #bbb;
}

/* 日期时间选择器 */
.weui-date-time-picker {
  display: flex;
  flex-direction: column;
  gap: 10px;
}
.weui-date-time-picker .weui-picker {
  height: 2.58823529em;
  line-height: 2.58823529em;
  padding-left: 10px;
  border: 1px solid #d9d9d9;
  border-radius: 4px;
  background-color: #f8f8f8;
}

/* 提示信息 */
.weui-toptips_warn {
  background-color: #E64340;
  color: #FFFFFF;
  padding: 5px;
  text-align: center;
  font-size: 14px;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 1000;
}
